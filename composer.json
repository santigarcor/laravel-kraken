{
  "name": "santigarcor/laravel-kraken",
  "description": "A minimal service provider to set up and use the Kraken API PHP library in Laravel 5",
  "keywords": ["laravel", "kraken"],
  "license": "MIT",
  "require": {
    "laravel/framework": ">=5.5",
    "kraken-io/kraken-php": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Santigarcor\\Kraken\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Santigarcor\\Kraken\\KrakenServiceProvider"
      ],
      "aliases": {
        "KrakenIO": "Santigarcor\\Kraken\\KrakenFacade"
      }
    }
  }
}
